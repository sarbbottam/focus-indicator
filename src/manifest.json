{
  "manifest_version": 2,

  "name": "Focus Indicator",
  "description": "Display focus indicator for the active intractable element in a webpage.",
  "version": "1.0.4",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["style.css"]
    }
  ]
}
